{
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "javascript.preferences.importModuleSpecifier": "non-relative"
  },
  "[json]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.fixAll.sortJSON": true,
      "source.formatOnSave": true
    },
    "editor.defaultFormatter": "richie5um2.vscode-sort-json"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.wordBasedSuggestions": false
  },
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "off",
  "customizeUI.activityBar": "bottom",
  "customizeUI.activityBarHideSettings": true,
  "customizeUI.statusBarPosition": "bottom",
  "customizeUI.stylesheet": {
    ".actions-container[aria-label='Editor actions'] .action-item .action-label:not([title='More Actions...'])": "display: none !important;"
  },
  "customizeUI.titleBar": "frameless",
  "debug.console.closeOnEnd": true,
  "debug.focusWindowOnBreak": false,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "debug.openDebug": "neverOpen",
  "debug.openExplorerOnEnd": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.formatOnSave": true,
    "source.organizeImports": true
  },
  "editor.cursorBlinking": "phase",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.folding": false,
  "editor.fontFamily": "JetBrainsMono Nerd Font, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 11,
  "editor.formatOnSave": true,
  "editor.glyphMargin": true,
  "editor.hover.above": false,
  "editor.hover.sticky": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "other": "on",
    "strings": "on"
  },
  "editor.renderWhitespace": "none",
  "editor.suggest.insertMode": "replace",
  "editor.tabCompletion": "on",
  "editor.wordWrap": "off",
  "errorLens.removeLinebreaks": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "gitlens.blame.avatars": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.heatmap.toggleMode": "file",
  "gitlens.hovers.avatars": false,
  "go.formatTool": "gofumports",
  "go.lintOnSave": "file",
  "go.lintTool": "revive",
  "gopls": {
    "formatting.gofumpt": true,
    "ui.diagnostic.analyses": {
      "nilness": true,
      "shadow": true,
      "unusedparams": true,
      "unusedwrite": true,
      "useany": true
    },
    "ui.diagnostic.staticcheck": true,
    "ui.semanticTokens": true
  },
  "json.format.enable": true,
  "oneDarkPro.vivid": true,
  "problems.showCurrentInStatus": true,
  "redhat.telemetry.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.gpuAcceleration": "off",
  "testing.automaticallyOpenPeekView": "never",
  "testing.openTesting": "openOnTestFailure",
  "typescriptHero.imports.organizeOnSave": true,
  "vscode-neovim.logLevel": "debug",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.vim/custom/vscode.vim",
  "vscode-neovim.revealCursorScrollLine": true,
  "vscode-neovim.textDecorationsAtTop": true,
  "window.autoDetectColorScheme": true,
  "window.newWindowDimensions": "maximized",
  "window.titleBarStyle": "native",
  "workbench.colorCustomizations": {
    // "editor.background": "#111111"
  },
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.editor.restoreViewState": true,
  "workbench.editor.showTabs": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editorAssociations": {
    "*.plist": "default"
  },
  "workbench.preferredDarkColorTheme": "GitHub Dark Default",
  "workbench.preferredHighContrastColorTheme": "GitHub Dark High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "GitHub Light Default",
  "workbench.preferredLightColorTheme": "GitHub Light Default"
}
